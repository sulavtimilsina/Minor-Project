<!DOCTYPE html>
<html>
    <head>
        <style>
        table {
            border-collapse: collapse;
        }
        table, th, td {
            border: 1px solid black;
        }
        </style>
    </head>
    <body>
        <div style="width: 85%; margin: 0 7.5%;">
            <div style="text-align: center; padding-left: 50px;">            
                <h3>Student Report</h5>
                <table>
                <% if (students.length > 0) { %>
                    <% students.forEach(student => {%>
                       <tr><td><%= student.username %></td><td><%= student.email %></td><td><%= student.startTime %></td><td><%=student.endTime %></td></tr>
                    <%  });%>
                <% }%>
                </table>
             </div>
        </div>
    </body>
</html>


